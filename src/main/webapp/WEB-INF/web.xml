<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" metadata-complete="false" version="3.1">
    <display-name></display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <description></description>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>com.reimbursement.main.Servlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>*.change</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>JSONMasterServlet</servlet-name>
        <servlet-class>com.reimbursement.main.JSONMasterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JSONMasterServlet</servlet-name>
        <url-pattern>*.json</url-pattern>
    </servlet-mapping>
</web-app>